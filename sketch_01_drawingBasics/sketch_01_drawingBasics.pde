
// how big is the drawing area? width + height
size(100, 100);


// Red, Green, Blue 0-255
background(200,200,200);


//line (x1, y1, x2, y2)
line(0, 0, 0, 0);


// rect (x, y, w, h) where x, y is top left
rect(0, 0, 0, 0);


// red, green, blue 0-255
fill(0, 0, 0);

// 0-255 grayscale
stroke(0);

// ellipse(x, y, w, h) where x, y is in the center
ellipse(0, 0, 0, 0);