// how big is the drawing area? width + height
size(600, 500);

// Red, Green, Blue 0-255
background(0,230,0);

//line (x1, y1, x2, y2)
line(30, 70, 230, 270);

// rect (x, y, w, h) where x, y is top left
rect(60, 70, 200, 300);

// red, green, blue 0-255
fill(255,0, 255);

// 0-255 grayscale
stroke(230);

// ellipse(x, y, w, h) where x, y is in the center
ellipse(250, 300, 200, 200);